    <script>
        $(function () {
            var socket = io();
            var format = function date2str(x, y) {
                var z = {
                    M: x.getMonth() + 1,
                    d: x.getDate(),
                    h: x.getHours(),
                    m: x.getMinutes(),
                    s: x.getSeconds()
                };
                y = y.replace(/(M+|d+|h+|m+|s+)/g, function (v) {
                    return ((v.length > 1 ? "0" : "") + eval('z.' + v.slice(-1))).slice(-2)
                });

                return y.replace(/(y+)/g, function (v) {
                    return x.getFullYear().toString().slice(-v.length)
                });
            }

            function emitChatMessage(socket) {
                var date = format(new Date(), 'dd/MM/yyyy hh:mm');
                var usersMsg = $('#chat-message-input').val();
                var myObj = {
                    message: $('#chat-message-input').val(),
                    username: "<%=user.username%>",
                    userid: "<%user._id%>",
                    date: format(new Date(), 'dd/MM/yyyy'),
                    time: format(new Date(), "hh:mm")
                };
                var emitMessageBody =
                    "<span style='border-bottom: 1px solid #BDBDBD; background-color: #eee; margin: 5px; display: block; padding:5px;'>" +
                    usersMsg + "<br><small><B><%=user.username%></B> " + date +
                    "</small></span>";
                socket.emit('chat message', emitMessageBody);
                $('#chat-message-input').val('');
                return false;
            }

            $('#chat-message-input').keypress(function (e) {
                if (e.which == 13) {
                    emitChatMessage(socket);
                }
            });


            socket.on('chat message', function (msg) {
                $('.show-messages').append($('<p>').html(msg));
                $('.show-messages').parent(".widget-inside").animate({
                    scrollTop: $('.show-messages').prop('scrollHeight')
                });
            });
        });
    </script>
</head>

